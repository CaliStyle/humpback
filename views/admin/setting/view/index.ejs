<!--admin/settings/view/index.ejs-->
<div class="grid-block">
	<div class="grid-block small-12 medium-8 large-9">
		<div class="grid-content">
			<h1>
				{{ thissetting.title || 'New Setting'}}
			</h1>
			<p class="lead">
				{{ thissetting.description }}
			</p>
			<div ng-repeat="item in setting.model.selected.attributes | toArray: true">

				<div 
					ng-api-input="item" 
					api-model="setting.model.selected"
					api-label="item.$key"
					api-input="thissetting[item.$key]"
				>
				</div>
			</div>
		</div>
	</div>
	<div class="grid-block small-12 medium-4 large-3" ui-view="widgets">
		
	</div>
</div>